    "dist/*"
  ],
  "scripts": {
    "build": "tsup-node src/index.ts src/readConfig.ts --dts --format cjs",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test:integration": "jest --config jest.integration.config.js"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.44.0",
    "chokidar": "^3.5.3",
    "semver": "^7.3.8",
    "synckit": "^0.8.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/fs-extra": "^9.0.13",
    "@typescript-eslint/types": "^5.44.0",
    "eslint": "^8.19.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.5.0"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "@remix-run/dev": "^1.5.1",
    "eslint": ">=7"
  },
  "license": "ISC"
}
